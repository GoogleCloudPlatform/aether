      (NAME Result)
      (VARIANTS
        (VARIANT Ok (HOLDS INTEGER))
        (VARIANT Error (HOLDS STRING))))
    
    (DEFINE_FUNCTION
      (NAME main)
      (RETURNS INTEGER)
