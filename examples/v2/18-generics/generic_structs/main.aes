// Generic Structs Example
// Demonstrates generic type parameters on struct definitions
// Note: This demonstrates V2 syntax parsing; full generic instantiation is WIP

module generic_structs;

// Simple container with one type parameter
struct Box<T> {
    value: T,
}

// Pair with two type parameters
struct Pair<T, U> {
    first: T,
    second: U,
}

// Stack-like structure (conceptual)
struct Stack<T> {
    data: T,
    size: Int,
}

// Key-value entry for maps
struct Entry<K, V> {
    key: K,
    value: V,
}

