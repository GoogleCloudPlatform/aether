// String Operations Example
// Demonstrates string manipulation

module string_operations;

@extern("C")
func strlen(s: Pointer<Char>) -> Int;

@extern("C")
func puts(s: Pointer<Char>) -> Int;

func main() -> Int {
    let message: String = "Hello, AetherScript!";

    // Get string length via FFI
    let len: Int = strlen("test");

    puts("String operations example");

    return len;
}
