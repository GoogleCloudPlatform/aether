// For-Each Example
// Demonstrates for-each loops and ranges

module foreach;

func main() -> Int {
    var sum: Int = 0;

    // For loop with exclusive range (0..10 means 0,1,2,...,9)
    for i in 0..10 {
        sum = {sum + i};
    }

    // For loop with inclusive range (0..=5 means 0,1,2,3,4,5)
    var count: Int = 0;
    for j in 0..=5 {
        count = {count + 1};
    }

    // sum = 0+1+2+...+9 = 45
    // count = 6
    return sum;
}
